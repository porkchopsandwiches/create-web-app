{
    "name": "@porkchopsandwich/create-web-app",
    "version": "1.0.0",
    "description": "NPX command to spin up a new Next.js app with ESLint, TypeScript, Prettier, etc. ready to go.",
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc",
        "lint": "eslint -f ./vendor/stylishSuccess.js --ext .js,.ts,.tsx .",
        "lint:fix": "eslint -f ./vendor/stylishSuccess.js --ext .js,.ts,.tsx . --fix",
        "execute": "ts-node ./src/index.ts"
    },
    "bin": "dist/index.js",
    "keywords": [
        "create",
        "app",
        "next",
        "recoil",
        "eslint",
        "typescript"
    ],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "chalk": "^4.1.0",
        "commander": "^7.1.0",
        "inquirer": "^8.0.0",
        "is-relative": "^1.0.0",
        "mkdirp": "^1.0.4",
        "p-each-series": "^2.2.0",
        "validate-npm-package-name": "^3.0.0"
    },
    "devDependencies": {
        "@types/inquirer": "^7.3.1",
        "@types/is-relative": "^1.0.0",
        "@types/mkdirp": "^1.0.1",
        "@types/node": "^14.14.31",
        "@types/validate-npm-package-name": "^3.0.2",
        "@typescript-eslint/eslint-plugin": "^4.16.1",
        "@typescript-eslint/parser": "^4.16.1",
        "eslint": "^7.21.0",
        "eslint-config-prettier": "^8.1.0",
        "eslint-plugin-prettier": "^3.3.1",
        "eslint-plugin-unicorn": "^28.0.2",
        "prettier": "^2.2.1",
        "ts-node": "^9.1.1",
        "typescript": "^4.2.3"
    }
}
